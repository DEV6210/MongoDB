{
    "anon-user": {
        "name": "anon-user",
        "type": "anon-user",
        "disabled": false
    },
    "api-key": {
        "name": "api-key",
        "type": "api-key",
        "disabled": true
    },
    "custom-function": {
        "name": "custom-function",
        "type": "custom-function",
        "config": {
            "authFunctionName": "customAuthFunc"
        },
        "disabled": true
    },
    "custom-token": {
        "name": "custom-token",
        "type": "custom-token",
        "config": {
            "audience": [],
