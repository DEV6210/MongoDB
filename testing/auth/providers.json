    "local-userpass": {
        "name": "local-userpass",
        "type": "local-userpass",
        "config": {
            "autoConfirm": true,
            "confirmEmailSubject": "Password Reset Confirmation",
            "confirmationFunctionName": "confirmFunc",
            "emailConfirmationUrl": "https://admin.theclink.in/confirmation",
            "resetFunctionName": "resetFunc",
            "resetPasswordSubject": "Password Reset",
            "resetPasswordUrl": "https://admin.theclink.in/passwordReset",
            "runConfirmationFunction": false,
            "runResetFunction": true
        },
        "disabled": false
